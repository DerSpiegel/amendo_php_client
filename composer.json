{
  "name": "der-spiegel/amendo-client",
  "description": "PHP client for OneVision Amendo https://www.onevision.com/solutions/image-editing/amendo/",
  "homepage": "https://github.com/DerSpiegel/amendo_php_client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Strehle",
      "homepage": "https://github.com/tistre",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2 <8.4",
    "ext-dom": "*",
    "guzzlehttp/guzzle": ">=6.3 <8.0",
    "psr/log": ">=1.1"
  },
  "require-dev": {
    "monolog/monolog": "^3.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "DerSpiegel\\AmendoClient\\": "src/"
    }
  }
}
